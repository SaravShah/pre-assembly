#! /usr/bin/env ruby

require File.expand_path(File.dirname(__FILE__) + '/../config/boot')

druids = [
  'druid:xx111yy2222',
  'druid:aa111bb2222',
]

druids.each do |druid|
	puts "Deleting #{druid}."
	begin
	  dobj = Dor::Config.fedora.client["objects/#{druid}"]
	  dobj.delete
	rescue
	  puts "an error occurred trying to delete #{druid}"
	end
end
